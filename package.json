{
  "name": "react-portfolio",
  "version": "1.0.0",
  "description": "Personal Portfolio",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "deploy": "git push heroku main:main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheBluWiz/react-portfolio.git"
  },
  "author": "Jamey Wicklund",
  "bugs": {
    "url": "https://github.com/TheBluWiz/react-portfolio/issues"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
